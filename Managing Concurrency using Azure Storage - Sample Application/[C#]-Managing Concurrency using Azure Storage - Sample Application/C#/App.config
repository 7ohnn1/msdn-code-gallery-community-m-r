<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
    <!-- Update this to include your Storage Account Name and Key -->
    <appSettings>
      <add key="StorageConnectionString" value="AccountName=[storageaccount];AccountKey=[accountkey];DefaultEndpointsProtocol=http;" />
    </appSettings>

</configuration>