<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ConfigSettings" type="Microsoft_Office_365_Service_Health_Watcher.AppConfig, Microsoft Office 365 Service Health Watcher"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <ConfigSettings
      ServiceURL="https://office365servicehealthcommunications.cloudapp.net/shdtenantcommunications.svc"
      DomainNames=""
      UserName=""
      Password=""
      PollInterval="900"
      PastDays="30"
      FreshnessThresholdDays="7"
      AlertMessageCenterEvents="1"
      AlertPlannedMaintenanceEvents="1"
      AlertServiceIncidentEvents="1"
      />
</configuration>